{
  "name": "super-admin-maintenance-panel",
  "version": "1.0.0",
  "description": "Super Admin Maintenance Control Panel for Multi-College SaaS",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "test": "node .github/workflows/test.js",
    "check": "node deployment-helper.js",
    "monitor": "node deployment-monitor.js start",
    "monitor:status": "node deployment-monitor.js status",
    "deploy:check": "node deployment-helper.js && npm run check",
    "deploy:production": "git add . && git commit -m 'Production deployment' && git push origin main"
  },
  "keywords": ["admin", "saas", "maintenance", "college"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "8.5.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-validator": "^7.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
